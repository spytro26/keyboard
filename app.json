{
  "expo": {
    "name": "Enzo Calc",
    "slug": "enzo-cool-calc",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/engo.png",
    "scheme": "enzocoolcalc",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/images/engo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "assetBundlePatterns": [
      "**/*"
    ],

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.enzo.coolcalc",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "5",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app allows users to select images from their photo library when needed."
      }
    },

    "android": {
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/engo.png",
        "backgroundColor": "#2563eb"
      },
      "package": "com.enzo.coolcalc",
      "googleServicesFile": "./google-services.json"
    },

    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-asset"
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "79e0519e-6141-4b87-b573-79eac610d15e"
      }
    },

    "owner": "spytro26",

    "runtimeVersion": "1.0.2"
  }
}
