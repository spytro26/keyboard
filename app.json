{
  "expo": {
    "name": "Enzo Cool Calc",
    "slug": "enzo-cool-calc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/harsh.png",
    "scheme": "enzocoolcalc",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/harsh.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.enzo.coolcalc",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/harsh.png",
        "backgroundColor": "#2563eb"
      },
      "package": "com.enzo.coolcalc",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-asset",
      [
        "expo-firebase-core",
        {
          "androidGoogleServicesFile": "./google-services.json",
          "iosGoogleServicesFile": "./GoogleService-Info.plist"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "79e0519e-6141-4b87-b573-79eac610d15e"
      },
      "router": {}
    },
    "owner": "spytro26",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/79e0519e-6141-4b87-b573-79eac610d15e"
    }
  }
}