{
  "expo": {
    "name": "Enzo Calc",
    "slug": "enzo-cool-calc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/engo.png",
    "scheme": "enzocoolcalc",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/engo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.enzo.coolcalc",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/engo.png",
        "backgroundColor": "#2563eb"
      },
      "package": "com.enzo.coolcalc",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "23630e06-6705-4770-b488-902f4c61fd6d"
      },
      "router": {}
    },
    "owner": "spytro726",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/23630e06-6705-4770-b488-902f4c61fd6d"
    }
  }
}
